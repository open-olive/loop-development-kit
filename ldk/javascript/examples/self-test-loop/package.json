{
  "name": "self-test-loop",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "webpack",
    "build-ldk": "cd ../.. && npm run build",
    "build-all": "npm run build-ldk && npm i && npm run build"
  },
  "keywords": [],
  "author": "",
  "ldk": {
    "configSchema": {
      "type": "object",
      "properties": {
        "testString": {
          "type": "string",
          "default": "testing a string"
        },
        "testNumber": {
          "type": "number",
          "default": 123
        },
        "testBoolean": {
          "type": "boolean",
          "default": true
        },
        "testStringEnum": {
          "enum": [
            "this",
            "is",
            "sparta"
          ]
        },
        "testNumberEnum": {
          "enum": [
            1,
            2,
            3
          ]
        },
        "testStringArray": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "uniqueItems": true,
          "default": []
        },
        "testNumberArray": {
          "type": "array",
          "items": {
            "type": "number"
          },
          "uniqueItems": true,
          "default": []
        },
        "testObject": {
          "type": "object",
          "properties": {
            "foo": {
              "type": "string",
              "default": "testing an object"
            },
            "bar": {
              "type": "string"
            }
          }
        }
      }
    },
    "permissions": {
      "browser": {
        "urlDomains": [
          {
            "value": "www.oliveai.dev"
          },
          {
            "value": "reactrouter.com/*"
          }
        ]
      },
      "clipboard": {},
      "cursor": {},
      "document": {},
      "network": {
        "urlDomains": [
          {
            "value": "html5rocks.websocket.org"
          },
          {
            "value": "api.fda.gov"
          },
          {
            "value": "catalog.data.gov"
          },
          {
            "value": "httpstat.us"
          },
          {
            "value": "127.0.0.1"
          },
          {
            "value": "github.com"
          },
          {
            "value": "demo.piesocket.com"
          }
        ]
      },
      "filesystem": {},
      "process": {},
      "ui": {},
      "user": {
        "optionalClaims": [
          {
            "value": "email"
          },
          {
            "value": "fullName"
          },
          {
            "value": "organizationId"
          },
          {
            "value": "organizationName"
          }
        ]
      },
      "system": {},
      "screen": {},
      "keyboard": {},
      "whisper": {},
      "vault": {},
      "window": {}
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-async-to-generator": "^7.13.0",
    "@babel/plugin-transform-destructuring": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@types/common-tags": "^1.8.0",
    "@types/terser-webpack-plugin": "^5.0.3",
    "babel-loader": "8.2.2",
    "ts-loader": "^8.3.0",
    "typescript": "^4.2.4",
    "webpack": "5.28.0",
    "webpack-cli": "4.5.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/runtime": "7.13.10",
    "@babel/runtime-corejs2": "7.13.10",
    "@oliveai/ldk": "file:../..",
    "babel-runtime": "6.26.0",
    "chance": "1.1.7",
    "common-tags": "^1.8.0",
    "core-js": "3.10.1",
    "es6-promise": "4.2.8",
    "react": "^17.0.2",
    "ts-node": "^9.1.1",
    "xlsx": "^0.17.0"
  }
}
